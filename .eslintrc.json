{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": 0,
        "no-return-assign": 0,
        "no-underscore-dangle": 0,
        "no-undef": 0,
        "no-use-before-define": 0,
        "prefer-promise-reject-errors": 0,
        "camelcase": 0
    }
}
